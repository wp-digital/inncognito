{
  "name": "innocode-digital/wp-inncognito",
  "version": "1.7.0",
  "type": "wordpress-muplugin",
  "description": "Login and Registration with user's AWS Cognito account.",
  "keywords": [
    "wordpress",
    "wp",
    "plugin",
    "mu-plugin",
    "aws",
    "cognito",
    "login",
    "registration",
    "oauth2"
  ],
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Innocode",
      "email": "post@innocode.no",
      "homepage": "https://innocode.com/"
    },
    {
      "name": "Viktor Kuliebiakin",
      "email": "kuliebiakin@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "firebase/php-jwt": "^6.0",
    "paragonie/sodium_compat": "^1.17",
    "endroid/qr-code": "^4.3",
    "aws/aws-sdk-php": "^3.222"
  },
  "require-dev": {
    "innocode-digital/php-cs": "^1.0",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "scripts": {
    "phpcs": "vendor/bin/phpcs --standard=phpcs.xml",
    "phpcbf": "vendor/bin/phpcbf --standard=phpcs.xml",
    "check-cs": "@phpcs",
    "fix-cs": "@phpcbf"
  },
  "autoload": {
    "psr-4": {
      "Innocode\\Cognito\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
